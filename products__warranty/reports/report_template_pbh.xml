<?xml version="1.0" encoding="utf-8"?>
<odoo>

<template id="report_pbh">
    <t t-call="web.basic_layout">
        <t t-foreach="docs" t-as="o">
           <div class="page">
               <div class="oe_structure"/>
                   <div class="col-xs-8">
                           <tr>
                              <td width="100%">
                                 <table>
                                      <tr>
                                          <td>
                                             <span>
                                                 <strong>Mã bảo hành:</strong>
                                             </span>
                                         </td>
                                         <td colspan="6" class="text-center">
                                             <span t-field="o.name"/>
                                         </td>
                                     </tr>
                                     <tr>
                                          <td>
                                             <span>
                                                 <strong>Tên khách hàng:</strong>
                                             </span>
                                         </td>
                                         <td colspan="6" class="text-center">
                                             <span t-field="o.namecus"/>
                                         </td>
                                     </tr>
                                       <tr>
                                          <td>
                                             <span>
                                                 <strong>Tên sản phẩm:</strong>
                                             </span>
                                         </td>
                                         <td colspan="6" class="text-center">
                                             <span t-field="o.product"/>
                                         </td>
                                     </tr>
                                      <tr>
                                          <td>
                                             <span>
                                                 <strong>Mã phiếu sửa:</strong>
                                             </span>
                                         </td>
                                         <td colspan="6" class="text-center">
                                             <span t-field="o.phieusua"/>
                                         </td>
                                     </tr>
                                      <tr>
                                           <td>
                                             <span>
                                                 <strong>Ngày kích hoạt:</strong>
                                             </span>
                                      </td>
                                         <td colspan="6" class="text-center">
                                             <span t-field="o.date"/>
                                         </td>
                                      </tr>
                                      <tr>
                                            <td>
                                             <span>
                                                 <strong>Hạn bảo hành:</strong>
                                             </span>
                                         </td>
                                         <td colspan="6" class="text-center">
                                             <span t-field="o.exp"/>
                                         </td>
                                       </tr>

                                        <tr>
                                            <td>
                                             <span>
                                                 <strong>Loại sản phẩm:</strong>
                                             </span>
                                         </td>
                                         <td colspan="6" class="text-center">
                                             <span t-field="o.loaisp"/>
                                         </td>
                                     </tr>

                               <tr>
                                   <td>
                                     <span>
                                    <strong>Đã kích hoạt:</strong>
                                  </span>
                                </td>
                               <td colspan="6" class="text-center">
                                <span t-field="o.active"/>
                                </td>
                         </tr>
                                       <tr>
                                   <td>
                                     <span>
                                    <strong>Ghi chú:</strong>
                                  </span>
                                </td>
                               <td colspan="6" class="text-center">
                                <span t-field="o.gc"/>
                                </td>
                         </tr>

                                 </table>
                              </td>
                           </tr>
                   </div>
               </div>
            <br/>


    </t>
     </t>
</template>
</odoo>